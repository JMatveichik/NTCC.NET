          </ctrls:ValveStateInlineCtrl.RenderTransform>
        </ctrls:ValveStateInlineCtrl>

        <!-- Подача воды в увлажнитель -->
        <ctrls:ValveInlineCtrl
          DataContext="{Binding DataPoints[YA11.OPN]}"
          Canvas.Top="710" 
